-- Scripts:

local function ZCHPI_fake_script() -- Minimize.LocalScript 
	local script = Instance.new('LocalScript', Minimize)

	local main_frame = script.Parent.Parent.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		if main_frame.Size.Height.Offset == 300 then
			main_frame:TweenSize(UDim2.new(0,500,0,40), Enum.EasingDirection.In, Enum.EasingStyle.Quad, .5)
		elseif main_frame.Size.Height.Offset == 40 then
			main_frame:TweenSize(UDim2.new(0,500,0,300), Enum.EasingDirection.In, Enum.EasingStyle.Quad, .5)
		end
	end)
end
coroutine.wrap(ZCHPI_fake_script)()
local function AGGSVCF_fake_script() -- MainFrame.Draggability 
	local script = Instance.new('LocalScript', MainFrame)

	script.Parent.Draggable = true
end
coroutine.wrap(AGGSVCF_fake_script)()
local function UTZRP_fake_script() -- highlight.LocalScript 
	local script = Instance.new('LocalScript', highlight)

	Enabled = false
	local Folder = Instance.new("Folder")
	Folder.Parent = game:GetService("CoreGui")
	
	script.Parent.MouseButton1Click:Connect(function()
		if Enabled == false then
			local Highlight = Instance.new("Highlight")
			Highlight.Parent = Folder
			Highlight.Name = _G.Player.Name
			Highlight.Adornee = _G.Player.Character
			Enabled = true
		else
			for Index,Player in pairs(game:GetService("CoreGui").Folder:GetChildren()) do
				Player:Destroy()
			end
			Enabled = false
		end
	end)
end
coroutine.wrap(UTZRP_fake_script)()
local function VPUKKX_fake_script() -- MeshlessHatTP.LocalScript 
	local script = Instance.new('LocalScript', MeshlessHatTP)

	local Toggle = false
	
	local function Config(Part)
		Part.AccessoryWeld:Destroy()
		Part.CanCollide = false
		if Part:FindFirstChild("Mesh") then
			Part:FindFirstChild("Mesh"):Destroy()
		elseif Part:FindFirstChild("SpecialMesh") then
			Part:FindFirstChild("SpecialMesh"):Destroy()
		end
		Part.Parent = game.Players.LocalPlayer.Character
	end
	
	local function CoFunc(Part)
		while true do
			task.wait()
			Part.CFrame = _G.Player.Character.Head.CFrame
			Part.RotVelocity = Vector3.new(10000, 10000, 10000)
		end
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		for _,object in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
			if object.ClassName == "Part" or object.ClassName == "MeshPart" then
				if object.Parent.ClassName == "Accessory" then
					Config(object)
					local co = coroutine.create(CoFunc(object))
					coroutine.resume(co)
				end
			end
		end
	end)
end
coroutine.wrap(VPUKKX_fake_script)()
local function KAVR_fake_script() -- ViewPlayer.LocalScript 
	local script = Instance.new('LocalScript', ViewPlayer)

	local Toggle = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if Toggle == false then
			workspace.Camera.CameraSubject = _G.Player.Character.Humanoid
			Toggle = true
		else 
			workspace.Camera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
			Toggle = false
		end
	end)
end
coroutine.wrap(KAVR_fake_script)()
local function PRAUYQ_fake_script() -- TP.LocalScript 
	local script = Instance.new('LocalScript', TP)

	script.Parent.MouseButton1Click:Connect(function()
		print("Teleport Executed")
		if _G.Player.Character:FindFirstChild("HumanoidRootPart") then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = _G.Player.Character.HumanoidRootPart.CFrame
		end
	end)
end
coroutine.wrap(PRAUYQ_fake_script)()
local function YRHFLO_fake_script() -- HatTP.LocalScript 
	local script = Instance.new('LocalScript', HatTP)

	local Toggle = false
	
	local function Config(Part)
		Part.AccessoryWeld:Destroy()
		Part.CanCollide = false
		Part.Parent = game.Players.LocalPlayer.Character
	end
	
	local function CoFunc(Part)
		while true do
			task.wait()
			Part.CFrame = _G.Player.Character.Head.CFrame * CFrame.new(0,.15,-.15)
			Part.RotVelocity = Vector3.new(10000, 10000, 10000)
		end
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
			if v.ClassName == "Part" or v.ClassName == "MeshPart" then
				if v.Parent.ClassName == "Accessory" then
					Config(v)
					local co = coroutine.create(CoFunc(v))
					coroutine.resume(co)
				end
			end
		end
	end)
end
coroutine.wrap(YRHFLO_fake_script)()
local function EWUTL_fake_script() -- Tool2Player.LocalScript 
	local script = Instance.new('LocalScript', Tool2Player)

	script.Parent.MouseButton1Click:Connect(function()
		local function Config(Part)
			Part.CanCollide = false
			if game.Players.LocalPlayer.Character:FindFirstChild("Right Arm") then
				game.Players.LocalPlayer.Character["Right Arm"]:FindFirstChild("RightGrip"):Destroy()
			else
				game.Players.LocalPlayer.Character["RightHand"]:FindFirstChild("RightGrip"):Destroy()
			end
		end
	
		local function CoFunc(Part)
			while true do
				task.wait()
				Part.CFrame = _G.Player.Character.Head.CFrame * CFrame.new(0,1.3,0)
				Part.RotVelocity = Vector3.new(10000, 10000, 10000)
			end
		end
	
		for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
			if v.ClassName == "Part" then
				if v.Parent.ClassName == "Tool" then
					Config(v)
					local co = coroutine.create(CoFunc(v))
					coroutine.resume(co)
				end
			end
		end
	end)
end
coroutine.wrap(EWUTL_fake_script)()
local function ICIBE_fake_script() -- BringPlayer.LocalScript 
	local script = Instance.new('LocalScript', BringPlayer)

	local Plr = game.Players.LocalPlayer.Character
	
	
	script.Parent.MouseButton1Click:Connect(function()
		if game.Players.LocalPlayer.Backpack:FindFirstChildOfClass("Tool") then
			local Pos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			local h = game.Players.LocalPlayer.Character.Humanoid:Clone()
			local plr = game.Players.LocalPlayer.Character
			local CF = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			local lp = game.Players.LocalPlayer
	
			h.Parent = game.Players.LocalPlayer.Character
			h.Name = "Hum"
	
			game.Players.LocalPlayer.Character.Humanoid:Destroy()
	
			for _,v in pairs(lp:GetDescendants()) do
				if v.ClassName == "Tool" then
					v.Parent = plr
				end
			end
	
			if game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"):FindFirstChildOfClass("Part") then
				firetouchinterest(game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"):FindFirstChildOfClass("Part"), _G.Player.Character.HumanoidRootPart, 0)
			else
				firetouchinterest(game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"):FindFirstChildOfClass("MeshPart"), _G.Player.Character.HumanoidRootPart, 0)
			end
		else
			print("This Script Needs Tools")
		end
	end)
end
coroutine.wrap(ICIBE_fake_script)()
local function LSAUOLL_fake_script() -- KillPlayer.LocalScript 
	local script = Instance.new('LocalScript', KillPlayer)

	script.Parent.MouseButton1Click:Connect(function()
		if game.Players.LocalPlayer.Backpack:FindFirstChildOfClass("Tool") then
			local h = game.Players.LocalPlayer.Character.Humanoid:Clone()
			local plr = game.Players.LocalPlayer.Character
			local CF = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			local lp = game.Players.LocalPlayer
	
			h.Parent = game.Players.LocalPlayer.Character
			h.Name = "Hum"
	
			game.Players.LocalPlayer.Character.Humanoid:Destroy()
	
			for _,v in pairs(lp:GetDescendants()) do
				if v.ClassName == "Tool" then
					v.Parent = plr
				end
			end
	
			if game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"):FindFirstChildOfClass("Part") then
				firetouchinterest(game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"):FindFirstChildOfClass("Part"), _G.Player.Character.HumanoidRootPart, 0)
			else
				firetouchinterest(game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"):FindFirstChildOfClass("MeshPart"), _G.Player.Character.HumanoidRootPart, 0)
			end
	
			plr.HumanoidRootPart.CFrame = CFrame.new(0,-100, 0)
		else
			print("This Script Needs Tools")
		end
	end)
end
coroutine.wrap(LSAUOLL_fake_script)()
local function VNMBLI_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	_G.Player = game.Players.LocalPlayer
	UIS = game:GetService("UserInputService")
	
	local function connect()
		for _,bttn in pairs(game:GetService("CoreGui").ScreenGui["Main Frame"]["Side bar"]["Player Scroll"]:GetChildren()) do
			if bttn:IsA("TextButton") then
				bttn.MouseButton1Click:Connect(function()
					_G.Player = game:GetService("Players")[bttn.Name]
				end)
			end
		end
	end
	
	local function Click(Instance)
		if Instance.Parent:FindFirstChildOfClass("Humanoid") then
			_G.Player = game.Players[Instance.Parent.Name]
		end
		
		if Instance.Parent.Parent:FindFirstChildOfClass("Humanoid") then
			_G.Player = game.Players[Instance.Parent.Name]
		end
	end
	
	for _,player in pairs(game:GetService("Players"):GetPlayers()) do
		clone = game:GetService("CoreGui").ScreenGui.TextButton:Clone()
		clone.Parent = game:GetService("CoreGui").ScreenGui["Main Frame"]["Side bar"]["Player Scroll"]
		clone.Name = player.Name
		clone.Text = player.Name
		clone.Visible = true
		connect()
	end
	
	game.Players.PlayerAdded:Connect(function(player)
		clone = game:GetService("CoreGui").ScreenGui.TextButton:Clone()
		clone.Parent = game:GetService("CoreGui").ScreenGui["Main Frame"]["Side bar"]["Player Scroll"]
		clone.Name = player.Name
		clone.Text = player.Name
		clone.Visible = true
		connect()
	end)
	
	game.Players.PlayerRemoving:Connect(function(player)
		game:GetService("CoreGui").ScreenGui["Main Frame"]["Side bar"]["Player Scroll"]:FindFirstChild(player.Name):Destroy()
	end)
	
	local Text = coroutine.create(function()
		while true do
			if _G.Player.Name ~= nil then
				game:GetService("CoreGui").ScreenGui["Main Frame"].Center.TextLabel.Text = _G.Player.Name
				wait()
			end
		end
	end)
	
	coroutine.resume(Text)
	
	local Image = coroutine.create(function()
		while true do 
			if _G.Player.Name ~= nil then
				game:GetService("CoreGui").ScreenGui["Main Frame"].Center.ImageLabel.Image = game:GetService("Players"):GetUserThumbnailAsync(_G.Player.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size100x100)
				wait()
			end
		end
	end)
	
	coroutine.resume(Image)
	
	UIS.InputBegan:Connect(function(key)
		if key.UserInputType == Enum.UserInputType.MouseButton1 then
			local target = game.Players.LocalPlayer:GetMouse().Target
			Click(target)
			print(target.Name)
		end
	end)
	
end
coroutine.wrap(VNMBLI_fake_script)()
